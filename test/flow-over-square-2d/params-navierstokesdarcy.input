[FreeFlow]
EnableUnsymmetrizedVelocityGradientForBeaversJoseph = false

[FreeFlow.Grid]
Verbosity = true
LowerLeft = 0 1
UpperRight = 1 2
Cells = 40 40
Grading1 = 1

[Darcy.Grid]
Verbosity = true
LowerLeft = 0 0
UpperRight = 1 1
Cells = 40 40
Grading1 = 1

[FreeFlow.Problem]
Name = navierstokes-iterative
EnableInertiaTerms = true
PressureDifference = 1e-4


[Darcy.Problem]
Name = darcy-iterative
InitialP = 0.0e-9

[Darcy.SpatialParams]
Permeability = 1e-6 # m^2
Porosity = 0.4
AlphaBeaversJoseph = 1.0

[Problem]
Name = fvca-iterative
EnableGravity = false
CouplingMode = ReconstructFreeFlowNormalStress

[Vtk]
AddVelocity = 1
